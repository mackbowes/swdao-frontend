{
  "name": "@project/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "clean": "rimraf build",
    "dist": "yarn build && yarn copyui && ncp .env.production build/.env && ncp src/resources build/resources",
    "dist:dev": "yarn build && yarn copyui:dev && ncp .env.test build/.env && ncp src/resources build/resources",
    "copyui": "rimraf build/public && cd ../react-app && yarn build && cd ../backend && ncp ../react-app/build build/public",
    "copyui:dev": "rimraf build/public && cd ../react-app && yarn build:dev && cd ../backend && ncp ../react-app/build build/public",
    "xxprestart": "yarn test",
    "start": "env-cmd -f .env.production nodemon ./build/bin/www",
    "start:dev": "env-cmd -f .env.test nodemon ./build/bin/www",
    "pretest": "yarn build",
    "test": "env-cmd -f .env.test jest --setupFiles dotenv/config",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.2",
    "@apollo/client": "^3.5.10",
    "@types/luxon": "^2.0.7",
    "@types/node-fetch": "^3.0.3",
    "@types/papaparse": "^5.3.1",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^3.6.3",
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "axios-request-throttle": "^1.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "debug": "~2.6.9",
    "ethers": "^5.4.6",
    "express": "~4.16.1",
    "express-openapi": "^10.1.0",
    "express-openapi-router": "^0.0.2",
    "express-validator": "^6.12.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.3.0",
    "graphql-config": "^4.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.2.0",
    "graphql-ws": "^5.7.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "lodash.uniq": "^4.5.0",
    "luxon": "^2.1.1",
    "morgan": "~1.9.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.1.1",
    "papaparse": "^5.3.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.3",
    "@types/moxios": "^0.4.12",
    "@types/node": "^16.9.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "axios-mock-adapter": "^1.20.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.0",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.12",
    "openapi-typescript": "^5.2.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^24.3.0",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  }
}
